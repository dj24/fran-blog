{
  "connection": "default",
  "collectionName": "posts",
  "info": {
    "name": "post",
    "description": "Blog post"
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "Content": {
      "type": "richtext"
    },
    "Title": {
      "type": "string"
    },
    "published": {
      "type": "boolean"
    },
    "categories": {
      "collection": "category",
      "via": "posts"
    },
    "Image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    }
  }
}